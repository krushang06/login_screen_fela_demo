<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="117.6dp"
        android:layout_marginBottom="-23.6dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/textlistbg"
        >
        <TextView
            android:id="@+id/favoritegroup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="38dp"
            android:layout_marginTop="32dp"
            android:elevation="10dp"
            android:fontFamily="@font/roboto_regular_400"
            android:gravity="start"
            android:maxLines="1"
            android:text="My Favorite Group 4"
            android:textColor="#11203A"
            android:textSize="17.49sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/cauntnumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="38dp"
            android:layout_marginTop="9dp"
            android:fontFamily="@font/roboto_regular_400"
            android:text="786"
            android:textColor="#7C8CA1"
            android:textSize="14sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/favoritegroup" />

        <TextView
            android:id="@+id/fixwords"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="9dp"
            android:fontFamily="@font/roboto_regular_400"
            android:text="Words"
            android:textColor="#7C8CA1"
            android:textSize="14sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/cauntnumber"
            app:layout_constraintTop_toBottomOf="@+id/favoritegroup" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>